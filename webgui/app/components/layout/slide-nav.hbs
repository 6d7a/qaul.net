<nav local-class="nav {{if @showNav "show"}}" ...attributes>
  <div aria-hidden="true" local-class="topbar {{if fakeIncomingOverlay "increase"}} {{if this.showAll "showall"}}">
    <button {{on "click" this.slideOut}}>
      {{fa-icon "angle-double-left"}}
    </button>
  </div>

  <div local-class="nav-overflow {{if this.showAll "showall"}}">
    <Nav @showAll="true" />
  </div>
</nav>